<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                   <resource id="Magento_Backend::stores_settings">
                       <resource id="Magento_Config::config">
                            <resource id="Magenest_Movie::config_magenest" title="Magenest Config" />
                       </resource>
                   </resource>
                </resource>
                <resource id="Magento_Backend::magenest_movie" title="Magenest Movie">
                    <resource id="Magenest_Movie::movie" title="Movie" sortOrder="60">
                        <resource id="Magenest_Movie::index" title="Director"/>
                        <resource id="Magenest_Movie::subscription" title="Actor"/>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>